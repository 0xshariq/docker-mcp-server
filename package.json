{
  "name": "@0xshariq/docker-mcp-server",
  "version": "2.0.0",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "docker-mcp-server": "docker-cli.js",
    "dms": "docker-cli.js",
    "dimages": "bin/basic/dimages.js",
    "dps": "bin/basic/dps.js",
    "dpsa": "bin/basic/dpsa.js",
    "dpull": "bin/basic/dpull.js",
    "drun": "bin/basic/drun.js",
    "dlogs": "bin/basic/dlogs.js",
    "dexec": "bin/basic/dexec.js",
    "dbuild": "bin/basic/dbuild.js",
    "dcompose": "bin/advanced/dcompose.js",
    "dup": "bin/advanced/dup.js",
    "ddown": "bin/advanced/ddown.js",
    "dnetwork": "bin/advanced/dnetwork.js",
    "dvolume": "bin/advanced/dvolume.js",
    "dinspect": "bin/advanced/dinspect.js",
    "dprune": "bin/advanced/dprune.js",
    "dlogin": "bin/advanced/dlogin.js",
    "dlogout": "bin/advanced/dlogout.js",
    "dpublish": "bin/advanced/dpublish.js",
    "dbridge": "bin/advanced/dbridge.js",
    "ddev": "bin/advanced/ddev.js",
    "dclean": "bin/advanced/dclean.js",
    "dstop": "bin/advanced/dstop.js",
    "dreset": "bin/advanced/dreset.js",
    "dlist": "bin/advanced/dlist.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "dev": "tsc -w & node dist/index.js",
    "dev:build": "tsc && node dist/index.js",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
    "clean": "rm -rf dist",
    "rebuild": "pnpm run clean && pnpm run build",
    "version": "echo $npm_package_version",
    "docker": "node docker-cli.js",
    "install:global": "pnpm run build && npm install -g .",
    "uninstall:global": "npm uninstall -g docker-mcp-server",
    "dev:install": "pnpm run build && pnpm run install:global",
    "start:mcp": "node dist/index.js"
  },
  "author": "Sharique Chaudhary",
  "license": "ISC",
  "description": "A comprehensive Model Context Protocol (MCP) server that provides Docker container and image management capabilities for AI assistants and automation tools. Includes 15 Docker operations + workflow combinations with complete CLI alias system, enhanced login/logout functionality, and advanced developer productivity tools.",
  "dependencies": {
    "@modelcontextprotocol/inspector": "^0.16.1",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "typescript": "^5.8.3"
  }
}
