{
  "name": "docker-logs",
  "aliases": ["dlogs"],
  "category": "Basic Container Operations",
  "description": "Fetch and follow logs from a Docker container",
  "usage": "docker-logs <containerId> [options]",
  "examples": [
    {
      "command": "dlogs mycontainer",
      "description": "Show all logs from container"
    },
    {
      "command": "dlogs mycontainer --tail 50",
      "description": "Show last 50 lines of logs"
    },
    {
      "command": "dlogs mycontainer --follow",
      "description": "Follow log output in real-time"
    },
    {
      "command": "dlogs mycontainer --since 1h",
      "description": "Show logs from last hour"
    },
    {
      "command": "dlogs --help",
      "description": "Show this help message"
    }
  ],
  "options": [
    {
      "flag": "-f, --follow",
      "description": "Follow log output (stream live logs)"
    },
    {
      "flag": "--tail <lines>",
      "description": "Number of lines to show from the end (default: all)"
    },
    {
      "flag": "--since <timestamp>",
      "description": "Show logs since timestamp (e.g. 2013-01-02T13:23:37Z) or relative (e.g. 42m for 42 minutes)"
    },
    {
      "flag": "--until <timestamp>",
      "description": "Show logs before timestamp"
    },
    {
      "flag": "-t, --timestamps",
      "description": "Show timestamps"
    },
    {
      "flag": "--details",
      "description": "Show extra details provided to logs"
    },
    {
      "flag": "-h, --help",
      "description": "Show this help message"
    }
  ],
  "time_formats": [
    "Absolute: 2013-01-02T13:23:37Z",
    "Relative: 42m (minutes), 1h (hours), 2d (days)"
  ],
  "notes": [
    "ðŸ’¡ Use --follow to stream live logs (Ctrl+C to exit)",
    "ðŸ”§ Combine --tail and --follow for recent logs with live updates",
    "ðŸ“Š Container must be running or have run to produce logs",
    "âš¡ Use container name or ID - both work"
  ]
}
