{
  "name": "docker-pull",
  "aliases": ["dpull"],
  "category": "Basic Image Operations",
  "description": "Pull a Docker image from a registry with platform and tag options",
  "usage": "docker-pull <imageName> [options]",
  "examples": [
    {
      "command": "dpull nginx:latest",
      "description": "Pull the latest nginx image"
    },
    {
      "command": "dpull ubuntu:20.04 --platform linux/amd64",
      "description": "Pull Ubuntu 20.04 for specific platform"
    },
    {
      "command": "dpull --all-tags nginx",
      "description": "Pull all tagged images in the nginx repository"
    },
    {
      "command": "dpull ghcr.io/owner/image:tag",
      "description": "Pull from GitHub Container Registry"
    },
    {
      "command": "dpull --help",
      "description": "Show this help message"
    }
  ],
  "options": [
    {
      "flag": "-a, --all-tags",
      "description": "Download all tagged images in the repository"
    },
    {
      "flag": "-q, --quiet",
      "description": "Suppress verbose output"
    },
    {
      "flag": "--platform <platform>",
      "description": "Set platform if server is multi-platform capable (e.g., linux/amd64, linux/arm64)"
    },
    {
      "flag": "--disable-content-trust",
      "description": "Skip image verification (default true)"
    },
    {
      "flag": "-h, --help",
      "description": "Show this help message"
    }
  ],
  "registries": [
    "docker.io (Docker Hub) - Default registry",
    "ghcr.io - GitHub Container Registry",
    "quay.io - Red Hat Quay",
    "gcr.io - Google Container Registry",
    "Your private registry"
  ],
  "notes": [
    "ðŸ’¡ If no tag is specified, 'latest' tag is assumed",
    "ðŸ”§ Use --platform to pull images for specific architectures",
    "ðŸ“¦ Private images require authentication with 'dlogin' first",
    "âš¡ Images are cached locally after first pull"
  ]
}
